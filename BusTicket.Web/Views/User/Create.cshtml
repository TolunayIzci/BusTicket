@{
}

<h1>User Add</h1>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Name" class="custom-control-label">Name</label>
                <input asp-for="Name" class="form-control" />
                <label asp-for="Surname" class="custom-control-label">Surname</label>
                <input asp-for="Surname" class="form-control" />
                <label asp-for="Email" class="custom-control-label">Email</label>
                <input asp-for="Email" class="form-control" />
                <label asp-for="Password" class="custom-control-label">Password</label>
                <input asp-for="Password" class="form-control" />
                <label asp-for="UserTypeId" class="custom-control-label">UserTypeId</label>
                <input asp-for="UserTypeId" class="form-control" />

            </div>
            <div class="form-group">
                <input type="submit" value="Add" class="btn-primary" />
            </div>

        </form>
    </div>
</div>